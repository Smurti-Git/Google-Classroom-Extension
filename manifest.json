{
  "manifest_version": 3,
  "name": "Google Classroom UI Enhancer",
  "version": "1.0",
  "description": "Adds a custom panel and UI to Google Classroom pages.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://classroom.google.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["scripts/custom.js"],
      "matches": ["https://classroom.google.com/*"]
    }
  ]
}
